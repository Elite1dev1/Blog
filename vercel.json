{
    "version": 2,
    "builds": [{
        "src": "cybershield/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "cybershield/wsgi.py"
        }
    ]
}
